{
  "name": "ellipsis",
  "version": "0.0.1",
  "description": "Ellipsis manages programmable bots",
  "main": "index.js",
  "private": true,
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "amdefine": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-jest": "^21.2.0",
    "babel-preset-react": "^6.24.1",
    "codemirror": "5.15.2",
    "config": "^1.29.2",
    "core-js": "2.4.1",
    "diff": "3.4.0",
    "eslint": "^4.9.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.57.3",
    "flow-typed": "^2.2.3",
    "javascript-debounce": "1.0.0",
    "jest": "^19.0.2",
    "moment": "2.14.1",
    "node-uuid": "^1.4.7",
    "react": "15.3.1",
    "react-addons-test-utils": "15.3.1",
    "react-dom": "15.3.1",
    "urijs": "1.18.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "whatwg-fetch": "1.0.0"
  },
  "scripts": {
    "lint": "scripts/run_eslint",
    "test": "jest --runInBand",
    "postinstall": "find ./npm_modules/* -maxdepth 2 -name package.json -exec sh -c 'cd $(dirname \"{}\"); npm install' \\;",
    "flow": "flow",
    "build": "NODE_CONFIG_DIR='./conf/webpack' webpack --env.WEBPACK_BUILD_PATH=${WEBPACK_BUILD_PATH} -p --config ./app/assets/frontend/webpack/webpack.base.config.js",
    "start": "NODE_CONFIG_DIR='./conf/webpack' WEBPACK_BUILD_PATH=${WEBPACK_BUILD_PATH} node ./app/assets/frontend/webpack/webpack.server"
  },
  "jest": {
    "globals": {
      "jsRoutes": {
        "controllers": {
          "APITokenController": {},
          "ApplicationController": {},
          "BehaviorEditorController": {},
          "ScheduledActionsController": {},
          "GithubConfigController": {},
          "SocialAuthController": {},
          "web": {
            "settings": {
              "IntegrationsController": {},
              "EnvironmentVariablesController": {},
              "RegionalSettingsController": {},
              "OAuth2ApplicationController": {},
              "AWSConfigController": {}
            }
          }
        }
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/project/target/.+",
      "<rootDir>/target/.+"
    ],
    "transform": {
      ".*": "<rootDir>/app/assets/javascripts/amdefinerator"
    },
    "testRegex": "((/__tests__/|/test/).*|(\\.|/)(test|spec))\\.jsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/.+",
      "/project/.+",
      "/target/.+",
      "/test/mocks/.+"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ellipsis-ai/ellipsis.git"
  },
  "author": "Ellipsis Co.",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ellipsis-ai/ellipsis/issues"
  },
  "homepage": "https://github.com/ellipsis-ai/ellipsis#readme"
}
