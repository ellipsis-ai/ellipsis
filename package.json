{
  "name": "ellipsis",
  "version": "0.0.1",
  "description": "Ellipsis manages programmable bots",
  "main": "index.js",
  "private": true,
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "npm": "^5.7.1"
  },
  "devDependencies": {
    "@types/jest": "^19.2.0",
    "@types/react": "^15.6.14",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "19.0.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "codemirror": "5.34.0",
    "config": "1.29.4",
    "core-js": "2.4.1",
    "diff": "3.4.0",
    "eslint": "4.17.0",
    "eslint-plugin-flowtype": "2.42.0",
    "eslint-plugin-react": "7.4.0",
    "flow-bin": "0.57.3",
    "flow-typed": "2.2.3",
    "javascript-debounce": "1.0.0",
    "jest": "19.0.2",
    "jshint": "2.9.5",
    "moment": "2.14.1",
    "react": "15.3.1",
    "react-addons-test-utils": "15.3.1",
    "react-dom": "15.3.1",
    "ts-jest": "^19.0.2",
    "ts-loader": "^3.5.0",
    "typescript": "^2.7.2",
    "urijs": "1.18.1",
    "uuid": "3.2.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1",
    "whatwg-fetch": "1.0.0"
  },
  "scripts": {
    "lint": "scripts/run_eslint",
    "test": "jest --runInBand",
    "postinstall": "find ./npm_modules/* -maxdepth 2 -name package.json -exec sh -c 'cd $(dirname \"{}\"); npm install' \\;",
    "flow": "flow",
    "build": "NODE_CONFIG_DIR='./conf/webpack' webpack --env.WEBPACK_BUILD_PATH=${WEBPACK_BUILD_PATH} -p --config ./app/assets/frontend/webpack/webpack.base.config.js",
    "start": "NODE_CONFIG_DIR='./conf/webpack' node ./app/assets/frontend/webpack/webpack.server",
    "typescript": "tsc --pretty --noEmit"
  },
  "jest": {
    "globals": {
      "jsRoutes": {
        "controllers": {
          "APITokenController": {},
          "ApplicationController": {},
          "BehaviorEditorController": {},
          "ScheduledActionsController": {},
          "GithubConfigController": {},
          "SocialAuthController": {},
          "web": {
            "settings": {
              "IntegrationsController": {},
              "EnvironmentVariablesController": {},
              "RegionalSettingsController": {},
              "OAuth2ApplicationController": {},
              "AWSConfigController": {}
            }
          }
        }
      },
      "ts-jest": {
        "tsConfigFile": "test/tsconfig.test.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/project/target/.+",
      "<rootDir>/target/.+"
    ],
    "testRegex": "((/__tests__/|/test/).*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/.+",
      "/project/.+",
      "/target/.+",
      "/test/mocks/.+"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ellipsis-ai/ellipsis.git"
  },
  "author": "Ellipsis Co.",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ellipsis-ai/ellipsis/issues"
  },
  "homepage": "https://github.com/ellipsis-ai/ellipsis#readme"
}
