{
  "name": "ellipsis",
  "version": "0.0.1",
  "description": "Ellipsis manages programmable bots",
  "main": "index.js",
  "private": true,
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "amdefine": "^1.0.0",
    "babel-core": "^6.13.2",
    "babel-jest": "^19.0.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "codemirror": "5.15.2",
    "core-js": "2.4.1",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "javascript-debounce": "1.0.0",
    "jest": "^19.0.2",
    "moment": "2.14.1",
    "node-uuid": "^1.4.7",
    "react": "15.3.1",
    "react-addons-test-utils": "15.3.1",
    "react-dom": "15.3.1",
    "urijs": "1.18.1",
    "whatwg-fetch": "1.0.0"
  },
  "scripts": {
    "lint": "scripts/run_eslint",
    "test": "jest --runInBand",
    "postinstall": "find ./npm_modules/* -maxdepth 2 -name package.json -exec sh -c 'cd $(dirname \"{}\"); npm install' \\;"
  },
  "jest": {
    "globals": {
      "jsRoutes": {
        "controllers": {
          "APITokenController": {},
          "ApplicationController": {},
          "BehaviorEditorController": {},
          "EnvironmentVariablesController": {},
          "OAuth2ApplicationController": {}
        }
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "Ben Newman"
    ],
    "transform": {
      ".*": "<rootDir>/app/assets/javascripts/amdefinerator"
    },
    "testRegex": "((/__tests__/|/test/).*|(\\.|/)(test|spec))\\.jsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/.+",
      "/project/.+",
      "/target/.+",
      "/test/mocks/.+"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ellipsis-ai/ellipsis.git"
  },
  "author": "Ellipsis Co.",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ellipsis-ai/ellipsis/issues"
  },
  "homepage": "https://github.com/ellipsis-ai/ellipsis#readme"
}
